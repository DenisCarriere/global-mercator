{
  "name": "global-mercator",
  "version": "1.7.0",
  "description": "Tools to help with TMS, Quadkey & Google (XYZ) Tiles",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "global-mercator.js",
    "global-mercator.min.js"
  ],
  "scripts": {
    "prepublish": "npm run test && npm run build",
    "test": "npm run lint && jest --coverage",
    "lint": "standard",
    "docs": "documentation readme index.js --section=API",
    "build": "browserify index.js -s mercator > global-mercator.js && uglifyjs global-mercator.js -c -m > global-mercator.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DenisCarriere/global-mercator.git"
  },
  "author": "Denis Carriere <carriere.denis@gmail.com>",
  "contributors": [
    "Denis Carriere <carriere.denis@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DenisCarriere/global-mercator/issues"
  },
  "homepage": "https://github.com/DenisCarriere/global-mercator#readme",
  "devDependencies": {
    "browserify": "^14.0.0",
    "coveralls": "^2.11.15",
    "documentation": "^4.0.0-beta.18",
    "jest-cli": "^18.0.0",
    "standard": "^8.6.0",
    "typescript": "^2.1.5",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "extent",
    "geojson",
    "google",
    "tms",
    "xyz",
    "tile",
    "slippy",
    "map",
    "latlng",
    "lnglat",
    "latitude",
    "longitude",
    "bbox",
    "pixels",
    "meters",
    "quadkey"
  ],
  "jest": {
    "verbose": true
  },
  "testEnvironment": "node",
  "standard": {
    "env": "jest"
  }
}
