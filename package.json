{
  "name": "global-mercator",
  "version": "2.5.0",
  "description": "Tools to help with TMS, Quadkey & Google (XYZ) Tiles",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "docs"
  ],
  "scripts": {
    "prepublish": "npm run test && npm run build && npm run docs",
    "pretest": "npm run lint",
    "test": "jest --coverage --verbose",
    "bench": "node bench.js",
    "lint": "standard index.js",
    "build": "npm run rollup && npm run uglify",
    "docs": "documentation readme index.js --shallow --section=API --markdown-toc",
    "rollup": "rollup -c rollup.config.js",
    "uglify": "uglifyjs docs/global-mercator.js -c -m > docs/global-mercator.min.js"
  },
  "author": "Denis Carriere <carriere.denis@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "extent",
    "geojson",
    "google",
    "tms",
    "xyz",
    "tile",
    "slippy",
    "map",
    "latlng",
    "lnglat",
    "latitude",
    "longitude",
    "bbox",
    "pixels",
    "meters",
    "quadkey"
  ],
  "devDependencies": {
    "benchmark": "^2.1.3",
    "documentation": "^4.0.0-beta.18",
    "jest-cli": "^19.0.2",
    "rollup": "^0.41.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.1.0",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "typescript": "^2.1.5",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {}
}
