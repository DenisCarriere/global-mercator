{
  "name": "global-mercator",
  "version": "2.0.1",
  "description": "Tools to help with TMS, Quadkey & Google (XYZ) Tiles",
  "main": "dist/global-mercator.js",
  "browser": "dist/global-mercator.browser.js",
  "module": "dist/global-mercator.es.js",
  "jsnext:main": "dist/global-mercator.es.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "dist"
  ],
  "scripts": {
    "prepublish": "npm run test && npm run uglify",
    "test": "npm run lint && npm run build && jest --coverage",
    "lint": "standard",
    "docs": "documentation readme index.js --section=API",
    "build": "rollup -c rollup.config.js",
    "uglify": "uglifyjs dist/global-mercator.browser.js -c -m -o dist/global-mercator.browser.min.js"
  },
  "author": "Denis Carriere <carriere.denis@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.11.15",
    "documentation": "^4.0.0-beta.18",
    "jest-cli": "^18.0.0",
    "rollup": "^0.41.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "standard": "^8.6.0",
    "typescript": "^2.1.5",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony"
  },
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "extent",
    "geojson",
    "google",
    "tms",
    "xyz",
    "tile",
    "slippy",
    "map",
    "latlng",
    "lnglat",
    "latitude",
    "longitude",
    "bbox",
    "pixels",
    "meters",
    "quadkey"
  ],
  "jest": {
    "verbose": true
  },
  "testEnvironment": "node",
  "standard": {
    "env": "jest"
  },
  "dependencies": {
    "global-mercator": "^1.9.2",
    "lodash.range": "^3.2.0"
  }
}
