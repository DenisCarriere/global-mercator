{
  "name": "global-mercator",
  "version": "3.0.0",
  "description": "Tools to help with TMS, Quadkey & Google (XYZ) Tiles",
  "repository": {
    "type": "git",
    "url": "git://github.com/DenisCarriere/global-mercator.git"
  },
  "main": "global-mercator.js",
  "module": "index.js",
  "jsnext:main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "global-mercator.js"
  ],
  "scripts": {
    "pretest": "standard",
    "test": "node -r @std/esm test.js",
    "postest": "rollup -f cjs -o global-mercator.js index.js",
    "bench": "node -r @std/esm bench.js",
    "docs": "documentation readme index.js --shallow --section=API"
  },
  "author": "Denis Carriere <@DenisCarriere>",
  "license": "MIT",
  "keywords": [
    "gis",
    "geo",
    "geojs",
    "geospatial",
    "geography",
    "geometry",
    "extent",
    "geojson",
    "google",
    "tms",
    "xyz",
    "tile",
    "slippy",
    "map",
    "latlng",
    "lnglat",
    "latitude",
    "longitude",
    "bbox",
    "pixels",
    "meters",
    "quadkey"
  ],
  "devDependencies": {
    "@mapbox/tilebelt": "*",
    "@std/esm": "*",
    "benchmark": "*",
    "documentation": "*",
    "standard": "*",
    "tape": "*"
  },
  "dependencies": {},
  "@std/esm": {
    "esm": "js",
    "cjs": true
  }
}
